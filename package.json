        "@types/jasmine": "2.6.0",
        "@types/node": "8.0.32",
        "chalk": "2.1.0",
        "codelyzer": "3.2.1",
        "core-js": "2.5.1",
        "jasmine-core": "2.8.0",
        "karma": "1.7.1",
